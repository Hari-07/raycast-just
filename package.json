{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "just-helper",
  "title": "Show a list of all just commands",
  "description": "A simple todo list.",
  "icon": "icon.png",
  "author": "hari",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Just Commands",
      "description": "Show a list of all Just Commands",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "1.31.0",
    "nanoid": "^3.3.1",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
